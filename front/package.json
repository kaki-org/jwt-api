{
  "name": "app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.12.2"
  },
  "packageManager": "pnpm@10.29.2",
  "scripts": {
    "dev": "nuxi dev",
    "dev:mock": "MOCK_SERVER=1 pnpm dev",
    "build": "pnpm merge-locales && nuxi build",
    "start": "nuxi start",
    "generate": "pnpm merge-locales && nuxi generate",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "merge-locales": "node scripts/merge-locales.js"
  },
  "dependencies": {
    "@intlify/core-base": "^11.0.0",
    "@intlify/shared": "^11.0.0",
    "@mdi/font": "^7.4.47",
    "@nuxtjs/i18n": "^10.0.3",
    "@pinia/nuxt": "^0.11.2",
    "core-js": "^3.33.3",
    "jwt-decode": "^4.0.0",
    "nuxt": "^4.0.0",
    "pinia": "^3.0.3",
    "pkg-types": "^2.2.0",
    "vue": "3.5.18",
    "vue-i18n": "^11.0.0",
    "vuetify": "^3.4.0",
    "webpack": "^5.101.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "node-gyp": "^12.0.0",
    "sass": "^1.70.0",
    "vue-tsc": "^3.0.5"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@parcel/watcher",
      "core-js"
    ],
    "onlyBuiltDependencies": [
      "esbuild",
      "unrs-resolver"
    ]
  }
}
